/**app.wxss**/

.dice-out {
  height: 220rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 50rpx;
}
.title1{
 line-height: 50px;
  font-size: 20px;
  color:white;
  text-align: center;
  background-color:slategrey; 
}
.box{
  border: none;
  height:100%;
}

page{
  height:100%;
}
.background {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
}
.tr{
  width:100%;
  display:flex;
  justify-content: space-evenly;
}
.th,.td{
  padding: 10px;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  width: 100%;
}
.th1,.td1{
  width: 10%;
}
